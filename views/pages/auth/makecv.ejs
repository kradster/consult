                function save() {
                    let params = $("input[type=text]");
                    let rad_buttons = $("input[type='radio'][name='details_gender']:checked");
                    for (let i = 0; i < rad_buttons.length; i++) {
                        params.push(rad_buttons[i]);
                    };
                    delete radios;
                    let dct = new Object();
                    for (let i = 0; i < params.length; i++) {
                        if (params[i].value) {
                            dct[params[i].id] = params[i].value;
                        }
                    }

                    dct.experience = [];
                    for (let i = 0; i < $('input[name=projectrole]').length; i++) {
                        if ($('select[name=projecttype]')[i].value == "" ||
                            $('input[name=projectrole]')[i].value == "" ||
                            $('input[name=projectinstitute]')[i].value == "" ||
                            $('input[name=projectdetails]')[i].value == "" ||
                            $('input[name=projectstartdate]')[i].value == "" ||
                            $('input[name=projectenddate]')[i].value == "")
                            continue;
                        dct.experience.push({
                            type: $('select[name=projecttype]')[i].value,
                            role: $('input[name=projectrole]')[i].value,
                            organization: $('input[name=projectinstitute]')[i].value,
                            description: $('input[name=projectdetails]')[i].value,
                            start_date: $('input[name=projectstartdate]')[i].value,
                            end_date: $('input[name=projectenddate]')[i].value
                        });
                    }

                    dct.skills = [];
                    for (let i = 0; i < $('input[name=skills]').length; i++) {
                        dct.skills.push($('input[name=skills]')[i].value);
                    }
                    console.log(dct);

                    $.ajax({
                        url: "/user/editcv",
                        method: "POST",
                        data: dct,
                        success: function(response) {
                            console.log(response);
                            array = response.messages
                            array.forEach(a => {
                                M.toast({
                                    html: "<b>" + a[0] + "</b>",
                                    classes: a[1]
                                });

                            });
                        },
                        error: function(error) {
                            console.log(error);
                        }
                    });
                }
