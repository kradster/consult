        <script>
            $(document).ready(function() {
                M.AutoInit();
                $('#GetStarted').on('click', function(e) {
                    e.preventDefault();
                    localStorage.setItem('name', $('#name').val());
                    localStorage.setItem('email', $('#email').val());
                    location.href = location.origin + "/signup "
                });
            });

            function subscribe() {
                let email = $("#email_val").val();
                console.log('asdf', $("#email").val());

                obj = {email: email};
                $.ajax({
                    url: window.location.origin + "/subscribe",
                    method: "POST",
                    data: obj,
                    success: function(response) {
                        console.log(response);
                        array = response.messages
                        array.forEach(a => {
                            M.toast({
                                html: "<b>" + a[0] + "</b>",
                                classes: a[1]
                            });

                        });

                    },
                    error: function(err) {
                        console.log(err);
                    }
                });
            }
            $('#email_submit').on('click', subscribe);
        </script>
